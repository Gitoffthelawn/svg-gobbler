{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "off",
    "no-restricted-syntax": "off",
    "no-alert": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
