{
  "name": "svg-gobbler",
  "version": "1.1.6",
  "description": "SVG Gobbler is a simple browser extension that highlights any available SVG content in your current window and lets you download it. The extension is available (soon) on the extension store.",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "brfs": "^2.0.2",
    "concurrently": "^4.1.1",
    "css-loader": "^2.1.1",
    "file-saver": "^2.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "onchange": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.18.2",
    "sass-loader": "^7.1.0",
    "svgo": "^1.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "transform-loader": "^0.2.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "gobbler": "concurrently \"webpack --config webpack.prod.js --watch\" \"onchange '**/*.(js|scss)' -- prettier --write {{changed}}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rossmoody/svg-gobbler.git"
  }
}
