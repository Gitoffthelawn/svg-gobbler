{
  "name": "svg-gobbler",
  "version": "1.3.0",
  "description": "SVG Gobbler is a simple browser extension that highlights any available SVG content in your current window and lets you download it. The extension is available (soon) on the extension store.",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-loader": "^8.1.0",
    "bestzip": "^2.1.5",
    "brfs": "^2.0.2",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "file-saver": "^2.0.2",
    "jszip": "3.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "onchange": "^6.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "2.0.5",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^3.0.3",
    "transform-loader": "^0.2.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "concurrently \"webpack --config webpack.js --watch\" \"onchange '**/*.(js|scss)' -- prettier --write {{changed}}\"",
    "zip": "bestzip package.zip extension/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rossmoody/svg-gobbler.git"
  }
}
