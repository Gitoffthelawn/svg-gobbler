{
  "name": "SVG Gobbler",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Ross Moody",
  "description": "Gobble all the SVGs on a web page.",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  },
  "browser_action": {
    "default_icon": "./img/icon128.png"
  },
  "permissions": ["activeTab"]
}
