{
  "name": "SVG Gobbler",
  "version": "0.1",
  "manifest_version": 2,
  "author": "Ross Moody",
  "description": "Gobble all the SVGs.",
  "background": {
    "scripts": [
      "./assets/scripts/background.js"
    ]
  },
  "icons": {
    "16": "./assets/img/icon16.png",
    "48": "./assets/img/icon48.png",
    "128": "./assets/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "./assets/img/icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}